	"license": "MIT",
    "authors": [
        {
            "name": "Adam Lavin",
            "email": "adam@lavoaster.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.0.x"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/Lavoaster/OAuth2Server/Exceptions/Exceptions.php",
            "src/migrations"
        ],
        "psr-0": {
            "Lavoaster\\OAuth2Server": "src/"
        }
    },
    "minimum-stability": "dev"
}